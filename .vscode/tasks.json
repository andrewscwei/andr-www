{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "label": "dev",
      "script": "dev",
      "isBackground": true,
      "problemMatcher": [
        {
          "owner": "vite",
          "fileLocation": [
            "relative",
            "${workspaceFolder}"
          ],
          "pattern": {
            "regexp": "NEVER MATCH THIS"
          },
          "background": {
            "activeOnStart": true,
            "beginsPattern": "^VITE",
            "endsPattern": "Local:"
          }
        }
      ]
    }
  ]
}
