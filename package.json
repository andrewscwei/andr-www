{
  "name": "andr-www",
  "version": "2.18.0",
  "private": true,
  "license": "MIT",
  "files": [
    "app/views",
    "config",
    "logs",
    "public",
    "previewer"
  ],
  "scripts": {
    "start": "node previewer",
    "test": "mocha tests --recursive --reporter spec",
    "clean": "gulp clean --gulpfile config/tasks.conf.js",
    "build": "gulp --gulpfile config/tasks.conf.js",
    "previewer": "NODE_ENV=development SSR_ENABLED=true npm run build && NODE_ENV=development DEBUG=app nodemon --verbose --watch previewer --watch config --ext js,json,pug --inspect previewer",
    "dev": "NODE_ENV=development npm run build -- --watch --serve",
    "prod": "NODE_ENV=production npm run build -- --serve",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "fs-extra": "^5.0.0",
    "i18n": "^0.8.3",
    "lodash": "^4.17.5",
    "mathjax-dom": "^0.3.0",
    "method-override": "^2.3.10",
    "moment": "^2.22.0",
    "morgan": "^1.9.0",
    "pluralize": "^7.0.0",
    "prism-dom": "^0.2.0",
    "pug": "^2.0.3",
    "require-dir": "^1.0.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "async": "^2.6.0",
    "chalk": "^2.3.2",
    "document-register-element": "^1.8.1",
    "eslint": "^4.19.1",
    "gsap": "1.20.4",
    "gulp": "^3.9.1",
    "gulp-prismic-mpa-builder": "^7.2.0",
    "hammerjs": "^2.0.8",
    "json-loader": "^0.5.7",
    "klaw": "^2.1.1",
    "meno": "^0.27.0",
    "minuet": "^1.3.0",
    "mocha": "^5.1.0",
    "nodemon": "^1.17.3",
    "page-manager": "^1.6.0",
    "prismjs": "^1.14.0",
    "pug-loader": "^2.4.0",
    "superagent": "^3.8.2",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": "8.9.4"
  }
