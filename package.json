    "fs-extra": "^0.30.0",
    "i18n": "^0.8.2",
    "jade": "^1.11.0",
    "lodash": "^4.13.1",
    "method-override": "^2.3.6",
    "moment": "^2.13.0",
    "morgan": "^1.7.0",
    "prismic.io": "^3.0.0",
    "require-dir": "^0.3.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "async": "^2.0.0-rc.6",
    "chalk": "^1.1.3",
    "gsap": "^1.18.5",
    "gulp-sys-metalprismic": "^2.0.2",
