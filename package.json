  "engines": {
    "node": "6.1.0"
  },
  "scripts": {
    "start": "node app",
    "test": "mocha tests --recursive --reporter spec",
    "clean": "gulp clean --gulpfile tasks/index.js",
    "build": "gulp --gulpfile tasks/index.js",
    "dev": "NODE_ENV=development npm run build -- --watch --serve",
    "stage": "NODE_ENV=staging npm run build -- --serve",
    "prod": "NODE_ENV=production npm run build -- --serve",
    "dev:server": "NODE_ENV=development npm run build && nodemon --ext js,json,jade app",
    "stage:server": "NODE_ENV=staging npm run build && node app",
    "prod:server": "NODE_ENV=production npm run build && node app"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
